#!/bin/bash
PK=/home/fusion809/GitHub/mine/packaging
pkgver=$(wget -cqO- https://github.com/SirCmpwn/sway/releases | grep "\.tar\.gz" | head -n 1 | cut -d '"' -f 2 | cut -d '/' -f 5 | sed 's/\.tar\.gz//g')
pkgverp=$(sed -n 's/pkgver=//p' $PK/sway/PKGBUILD | head -n 1 | sed 's/\.r/\-r/g')

echo "$pkgver" > /tmp/sway
echo "$pkgverp\n" >> /tmp/sway

if ! [[ "$pkgver" == "$pkgverp" ]]; then
    echo "S $pkgver"
else
    echo "S OK"
fi
