#!/bin/bash
PK=/home/fusion809/GitHub/mine/packaging
pkgver=$(wget -cqO- https://www.chemaxon.com/download/marvin-suite/ | grep "deb" | head -n 1 | cut -d '"' -f 14 | cut -d '/' -f 5)
pkgverp=$(sed -n 's/pkgver=//p' $PK/marvin/PKGBUILD | head -n 1 | sed 's/\.r/\-r/g')

echo "$pkgver" > /tmp/marvin
echo "$pkgverp\n" >> /tmp/marvin

if ! [[ "$pkgver" == "$pkgverp" ]]; then
    echo "M$pkgver"
else
    echo "M OK"
fi
